	<queryString>
		<![CDATA[SELECT
     tblkehadiran.`idpegawai` AS idpegawai,
     tblpegawai.`nama` AS nama,
     tbljabatan.`namajabatan` AS jabatan,
     tblkehadiran.`jmasuk` AS jam_masuk,
     tblkehadiran.`jkeluar` AS jam_keluar
FROM
     `tbljabatan` tbljabatan INNER JOIN `tblpegawai` tblpegawai ON tbljabatan.`idJabatan` = tblpegawai.`jabatan`
     INNER JOIN `tblkehadiran` tblkehadiran ON tblpegawai.`idpegawai` = tblkehadiran.`idpegawai`

WHERE
	tblkehadiran.tanggal=$P{tanggalhadir}]]>
	</queryString>
